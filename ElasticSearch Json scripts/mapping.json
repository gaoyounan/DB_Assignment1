{
    "busdb": {
        "mappings": {
            "_doc": {
                "properties": {
                    "arrival_time": {
                        "type": "date",
                        "format": "HH:mm:ss"
                    },
                    "departure_time": {
                        "type": "date",
                        "format": "HH:mm:ss"
                    },
                    "name_stop": {
                        "type": "keyword"
                    },
                    "route_id": {
                        "type": "keyword"
                    },
                    "service_id": {
                        "type": "keyword"
                    },
                    "shape_id": {
                        "type": "keyword"
                    },
                    "stop_sequence": {
                        "type": "long"
                    },
                    "trip_headsign": {
                        "type": "keyword"
                    },
                    "trip_id": {
                        "type": "keyword"
                    }
                }
            }
        }
    }
}