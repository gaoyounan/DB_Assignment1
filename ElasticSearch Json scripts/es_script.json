//B) GET bustime_search
{
    "_source": "route_id",
    "from": 0,
    "size": 1,
    "query": {
        "range": {
            "arrival_time": {
                "gte": "6:00:00.000",
                "lte": "23:59:59.000",
                "format": "HH:mm:ss.sss"
            }
        }
    },
    "aggs": {
        "by_rout": {
            "terms": {
                "field": "trip_headsign"
            }
        }
    }
}

